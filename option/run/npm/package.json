{
  "name": "zgxl-ios",
  "version": "1.0.0",
  "description": "规范commitmsg",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "commitmsg": "validate-commit-msg",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 -a"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.zgxl168.com:1888/zgxl/zgxl-ios.git"
  },
  "author": "Abyss",
  "license": "MIT",
  "devDependencies": {
    "cz-conventional-changelog": "^2.1.0",
    "cz-customizable": "^5.2.0",
    "husky": "^0.14.3",
    "validate-commit-msg": "^2.14.0"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "test",
        "chore",
        "temp"
      ]
    },
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {}
}
